{
    "files.alwaysInclude": [
      "map_locations/core.py",
      "map_locations/cli.py"
    ],
    "files.exclude": {
      "**/__pycache__": true,
      "**/*.pyc": true,
      "**/build": true,
      "**/dist": true,
      "**/*.egg-info": true,
      "**/.git": true,
      "**/.mypy_cache": true,
      "**/.pytest_cache": true,
      "**/maps/*.html": false,
      "**/maps/*.kml": false,
      "**/maps/*.geojson": false,
      "**/maps/*.csv": false,
      "**/maps/*.json": false,
      "**/maps/*.yaml": false,
      "**/maps/*.yml": false,
      "**/maps/*.jsonl": false
    },
    "git.confirmSync": true,
    "git.confirmPush": true,
    "git.confirmCommit": true,
    "git.enableSmartCommit": false,
    "git.autostage": false,
    "git.autoStash": false,
    "git.confirmDelete": true,
    "git.confirmCheckout": true,
    "git.confirmMerge": true,
    "git.confirmRebase": true,
    "git.confirmReset": true,
    "git.confirmRevert": true,
    "git.confirmStash": true,
    "git.confirmUnstage": true,
    "git.confirmDiscard": true
  }
